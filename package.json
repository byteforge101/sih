{
  "name": "with-tailwind",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start-sih": "cd apps/sih && npm run start",
    "db:migrate": "dotenv -e packages/prisma/prisma/.env -- npx prisma migrate dev",
    "db:reset": "dotenv -e packages/prisma/prisma/.env -- npx prisma migrate reset",
    "db:studio": "dotenv -e packages/prisma/prisma/.env -- npx prisma studio",
    "db:generate": "dotenv -e packages/prisma/prisma/.env -- npx prisma generate",
    "db:push": "dotenv -e packages/prisma/prisma/.env -- npx prisma db push"
  },
  "prisma": {
    "schema": "packages/prisma/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.15.0",
    "turbo": "^2.5.6"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
